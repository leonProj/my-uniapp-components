{
	"easycom": {
		"^u-(.*)": "uview-ui/components/u-$1/u-$1.vue"
	},
	"pages": [
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页"
			}
		}
	],
	"subPackages": [
		{
			"root": "pages/hook/",
			"pages": [
				{
					"path": "useRequest/index",
					"style": {
						"navigationBarTitleText": "useRequest"
					}
				}
			]
		},
		{
			"root": "pages/hookComponents/",
			"pages": [
				{
					"path": "TlaStateManager/index",
					"style": {
						"navigationBarTitleText": "TlaStateManager"
					}
				}
			]
		},
		{
			"root": "pages/template/",
			"pages": [
				{
					"path": "standardList/index",
					"style": {
						"navigationBarTitleText": "标准下拉列表"
					}
				}
			]
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	}
}
